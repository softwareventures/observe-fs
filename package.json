{
  "private": true,
  "name": "@softwareventures/observe-fs",
  "version": "0.0.0-development",
  "description": "",
  "keywords": [],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/observe-fs",
  "bugs": "https://github.com/softwareventures/observe-fs/issues",
  "repository": "github:softwareventures/observe-fs",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "commit": "cz",
    "fix": "tsc --noEmit && eslint . --fix && prettier --write .",
    "postinstall": "husky",
    "lint": "tsc --noEmit && eslint . && prettier --check .",
    "prepack": "tsc && pinst --disable",
    "postpack": "pinst --enable",
    "test": "ava"
  },
  "sideEffects": false,
  "exports": {
    ".": "./index.js"
  },
  "engines": {
    "node": "^18 || ^20 || >=21"
  },
  "dependencies": {
    "@types/node": "^18.0.0 || ^20.0.0 || >=21.0.0",
    "rxjs": "^6.0.0 || ^7.0.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "19.4.0",
    "@commitlint/config-conventional": "19.2.2",
    "@commitlint/cz-commitlint": "19.4.0",
    "@softwareventures/array": "7.0.0",
    "@softwareventures/eslint-config": "7.6.0",
    "@softwareventures/precise-commits": "4.0.15",
    "@softwareventures/prettier-config": "4.0.0",
    "@softwareventures/semantic-release-config": "4.1.0",
    "@softwareventures/tsconfig": "8.0.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "ava": "6.1.3",
    "commitizen": "4.3.0",
    "conventional-changelog-conventionalcommits": "7.0.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsdoc": "48.11.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-sonarjs": "1.0.4",
    "husky": "9.1.5",
    "inquirer": "9.3.6",
    "pinst": "3.0.0",
    "prettier": "3.3.2",
    "semantic-release": "23.1.1",
    "tempy": "3.1.0",
    "tsimp": "2.0.11",
    "typescript": "5.5.4"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@softwareventures"
  },
  "prettier": "@softwareventures/prettier-config",
  "config": {
    "commitizen": {
      "path": "./node_modules/@commitlint/cz-commitlint"
    }
  },
  "ava": {
    "concurrency": 1,
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--import=tsimp/import"
    ],
    "timeout": "40s"
  },
  "release": {
    "extends": "@softwareventures/semantic-release-config"
  },
  "packageManager": "yarn@4.4.0"
}
